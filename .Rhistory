library(random)
Problema_3<-function(Nrep,radio){ #Funcion que recive las repeticiones y el valor del radio
Dentro <- 0
PuntoX<-NULL
PuntoY<-NULL
PuntoZ<-NULL
for(i in 1:Nrep){
#Generador de puntos de cada repeticion
Punto<- c(runif(1,min = -1*radio,max = 1*radio),runif(1,min = -1*radio,max = 1*radio),runif(1,min = -1*radio,max=1*radio))
circunferencia <- Punto[1]^2+Punto[3]^2 #Valor del radio en el eje x,z
XCua=radio^2-Punto[2]^2 #Valor de x^2 en la fun de circunferencia
if (circunferencia<=XCua){ #Si cumple la condicion esta dentro del area desde el eje x,z
Dentro<-Dentro + 1
PuntoX<-c(PuntoX,Punto[1]) #Se alamacenan los puntos para graficar
PuntoY<-c(PuntoY,Punto[2])
PuntoZ<-c(PuntoZ,Punto[3])
}
}
Vol<-Dentro/(Nrep)*(radio*2)^3 #Puntos dentro/ Repeticiones * el volumen del cubo 4x4x4
print(paste0("El volumen en unidades cubicas es de ", Vol,"u^3"))
#plot3d(PuntoX,PuntoY,PuntoZ,col=rainbow(1000)) #Se imprime en 3d
}
Problema_3(10000,2) #Repeticiones y radio
